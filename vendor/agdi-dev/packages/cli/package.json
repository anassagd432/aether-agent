{
    "name": "agdi",
    "version": "3.3.5",
    "description": "AI-powered app generator - build full-stack apps from natural language in your terminal",
    "type": "module",
    "bin": {
        "agdi": "bin/agdi.js"
    },
    "main": "./dist/index.js",
    "exports": {
        ".": {
            "import": "./dist/index.js"
        }
    },
    "files": [
        "bin",
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format esm --clean",
        "dev": "tsx src/index.ts",
        "start": "tsx src/dev.ts",
        "test": "vitest run",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "cli",
        "ai",
        "code-generation",
        "app-generator",
        "llm",
        "gpt",
        "claude",
        "gemini",
        "react",
        "vite",
        "full-stack"
    ],
    "author": "Agdi Systems Inc.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/anassagd432/Agdi-dev.git"
    },
    "homepage": "https://agdi-dev.vercel.app",
    "bugs": {
        "url": "https://github.com/anassagd432/Agdi-dev/issues"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "devDependencies": {
        "@types/babel__traverse": "^7.28.0",
        "@types/better-sqlite3": "^7.6.13",
        "@types/figlet": "^1.7.0",
        "@types/file-saver": "^2.0.7",
        "@types/fs-extra": "^11.0.0",
        "@types/gradient-string": "^1.1.6",
        "@types/node": "^20.0.0",
        "@types/react": "^19.2.13",
        "@types/uuid": "^10.0.0",
        "tsup": "^8.0.0",
        "tsx": "^4.21.0",
        "typescript": "^5.4.0",
        "vitest": "^3.0.0"
    },
    "dependencies": {
        "@babel/core": "^7.28.6",
        "@babel/parser": "^7.28.6",
        "@babel/traverse": "^7.28.6",
        "@babel/types": "^7.28.6",
        "@google/genai": "^1.0.0",
        "@inquirer/prompts": "^8.2.0",
        "@xenova/transformers": "^2.17.2",
        "better-sqlite3": "^12.6.2",
        "boxen": "^8.0.1",
        "chalk": "^5.3.0",
        "commander": "^12.0.0",
        "figlet": "^1.9.4",
        "fs-extra": "^11.2.0",
        "gradient-string": "^3.0.0",
        "ink": "^6.6.0",
        "ink-big-text": "^2.0.0",
        "ink-gradient": "^3.0.0",
        "ink-spinner": "^5.0.0",
        "ink-text-input": "^6.0.0",
        "ink-use-stdout-dimensions": "^1.0.5",
        "jszip": "^3.10.0",
        "ora": "^8.0.0",
        "react": "^19.2.4",
        "ts-node": "^10.9.2",
        "uuid": "^13.0.0"
    }
}
